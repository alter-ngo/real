model Parinte {
  id               Int      @id @default(autoincrement())
  ref              String?  @db.VarChar(255)
  email            String   @unique @db.VarChar(255)
  age              Int
  location         String   @db.VarChar(255)
  hs               String   @db.VarChar(255)
